{
  "name": "autoprefixer",
  "version": "6.7.7",
  "description": "Parse CSS and add vendor prefixes to CSS rules using values from the Can I Use website",
  "keywords": [
    "autoprefixer",
    "css",
    "prefix",
    "postcss",
    "postcss-plugin"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "postcss/autoprefixer",
  "dependencies": {
    "babel-register": "^6.24.0",
    "browserslist": "^1.7.7",
    "caniuse-lite": "^0.2.0",
    "normalize-range": "^0.1.2",
    "num2fraction": "^1.2.2",
    "postcss": "^5.2.16",
    "postcss-value-parser": "^3.2.3"
  },
  "devDependencies": {
    "babel-preset-env": "^1.3.2",
    "browserify": "^14.3.0",
    "eslint-config-postcss": "^2.0.2",
    "fs-extra": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coffee": "^2.3.4",
    "gulp-eslint": "^3.0.1",
    "gulp-json-editor": "^2.2.1",
    "gulp-mocha": "^4.3.0",
    "gulp-replace": "^0.5.4",
    "mocha": "^3.2.0",
    "should": "^11.2.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "scripts": {
    "test": "gulp"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss",
    "rules": {
      "complexity": "off",
      "max-len": [2, 80, {
        "ignoreComments": true,
        "ignoreStrings": true
      }]
    }
  },
  "babel": {
    "presets": [
        ["env", {
            "targets": {
                "browsers": "IE 11",
                "node": 4
            },
            "loose": true
        }]
    ]
  }
}
